{
  "name": "customer-registration",
  "version": "1.0.15",
  "private": true,
  "scripts": {
    "start": "node app",
    "init": "npm ci && cd client && npm ci",
    "dev": "nodemon app",
    "serverstart": "DEBUG=customer-registration:* npm run dev",
    "audit": "npm audit --registry=https://registry.npmjs.org",
    "lint": "./node_modules/.bin/eslint .",
    "test:jenkins": "nyc mocha --timeout 100000 --reporter mocha-sonar-generic-test-coverage tests/*.js --exit",
    "test": "nyc --reporter=lcov mocha --timeout 100000 --reporter mocha-junit-reporter tests/*.js --exit",
    "build:docker": "docker build -t mss-nodejs-ms .",
    "start:docker": "docker run --env PORT=3000 -p 3000:3000 mss-nodejs-ms:latest"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "btoa": "^1.2.1",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-defaults": "^2.0.0",
    "eslint": "^7.5.0",
    "express": "~4.16.1",
    "express-prom-bundle": "^6.1.0",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-ci-oidc": "^2.0.4",
    "pino": "^6.5.1",
    "pino-caller": "^3.0.2",
    "pino-http": "^5.2.0",
    "pino-pretty": "^4.1.0",
    "prom-client": "^12.0.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.3.1",
    "eslint-plugin-jasmine": "^4.1.1",
    "mocha": "^8.0.1",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-sonar-generic-test-coverage": "^0.0.1",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sinon": "^11.1.0"
  }
}
